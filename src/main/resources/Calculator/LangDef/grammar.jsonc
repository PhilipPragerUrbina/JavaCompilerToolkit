{
  "start": "expression",
  "expression": {
    "match": [
      "term"
    ],
    "save": "expression"
  },
  "term": {
    "match": [
      "factor",
      {
        "repeating": {
          "match": [
            {
              "options": [
                "plus",
                "minus"
              ]
            },
            "factor"
          ]
        }
      }
    ],
    "save": "binary"
  },
  "factor": {
    "match": [
      "unary",
      {
        "repeating": {
          "match": [
            {
              "options": [
                "mul",
                "div"
              ]
            },
            "unary"
          ]
        }
      }
    ],
    "save": "binary"
  },
  "unary": {
    "options": [
      {
        "match": [
          "minus",
          "unary"
        ],
        "save": "unary"
      },
      "primary"
    ]
  },
  "primary": {
    "options": [
      {
        "match": [
          "integer"
        ],
        "save": "literal"
      },
      {
        "match": [
          "(",
          "expression",
          ")"
        ],
        "save": "group"
      }
    ]
  }
}
